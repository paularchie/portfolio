{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
      "esModuleInterop": true,
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "jsx": "react",
      "strictNullChecks": true,
      "noUnusedLocals": false,
      "isolatedModules": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true
  },
  "include": [
      "./src/**/*"
  ],
}