generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique @db.VarChar(30)
  email    String @unique @db.VarChar(40)
  password String @db.VarChar(100)
  role     Role   @default(USER)
}

enum Role {
  USER
  ADMIN
}
