generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int         @id @default(autoincrement())
  username String      @unique
  email    String      @unique
  password String
  roles    UserRoles[]
}

model UserRoles {
  id    Int    @id @default(autoincrement())
  name  String @unique
  users User[]
}
